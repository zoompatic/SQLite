CREATE TABLE behandlungen ( 
    "BehandlungsID" INT PRIMARY KEY NOT NULL,
    "ArztID" INT NOT NULL,
    "PatientenID" INT NOT NULL,
    "Diagnose" VARCHAR(500) NOT NULL,
    "Datum" DATE NOT NULL,
    FOREIGN KEY (ArztID) REFERENCES aerzte(PersonalID),
    FOREIGN KEY (PatientenID) REFERENCES patienten(PatientenID)
);
