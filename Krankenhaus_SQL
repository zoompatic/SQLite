-- In Zeile 1:
CREATE TABLE verschreibungen (
    "VerschreibungsID" INT PRIMARY KEY NOT NULL,
    "PatientID" INT NOT NULL,
    "ArztID" INT NOT NULL,
    "MedikamentenID" INT NOT NULL,
    "Startdatum" DATE NOT NULL,
    "Enddatum" DATE NOT NULL,
    FOREIGN KEY (PatientID) REFERENCES patienten(PatientenID),
    FOREIGN KEY (ArztID) REFERENCES aerzte(PersonalID),
    FOREIGN KEY (MedikamentenID) REFERENCES medikamente(MedikamentenID)
);
